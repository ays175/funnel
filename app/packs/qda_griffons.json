{
  "domain": "qda_griffons",
  "keywords": [
    "griffon", "qpv", "quartiers", "entrepreneur", "financement", "formation", 
    "accompagnement", "aide", "subvention", "prêt", "création", "entreprise",
    "développement", "croissance", "quartiers prioritaires", "prestataire",
    "fournisseur", "service", "comptable", "informatique", "marketing"
