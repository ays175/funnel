  "facets": [
    {
      "id": "search_type",
      "title": "What are you looking for?",
      "question": "What type of help do you need?",
      "keywords": ["search", "find", "looking", "need", "want"],
      "choices": [
        { "value": "Programs & Funding", "subchoices": ["Grants", "Loans", "Training", "Support programs"] },
        { "value": "Service Provider (QDA Member)", "subchoices": ["Find a provider", "Get quotes", "Recommendations"] }
      ],
      "default_value": "Programs & Funding"
    },
    {
      "id": "service_sector",
      "title": "Service sector needed",
      "question": "What type of service provider are you looking for?",
      "keywords": ["accountant", "IT", "marketing", "legal", "HR", "construction", "transport", "training", "cleaning", "catering", "telecom"],
      "choices": [
        { "value": "Accounting & Tax Advisory", "subchoices": [] },
        { "value": "IT & Digital Services", "subchoices": [] },
        { "value": "Marketing & Communication", "subchoices": [] },
        { "value": "Legal & Business Consulting", "subchoices": [] },
        { "value": "HR & Recruitment", "subchoices": [] },
        { "value": "Construction & Renovation", "subchoices": [] },
        { "value": "Transport & Logistics", "subchoices": [] },
        { "value": "Professional Training", "subchoices": [] },
        { "value": "Cleaning & Maintenance", "subchoices": [] },
        { "value": "Catering & Food Services", "subchoices": [] },
        { "value": "Telecommunications", "subchoices": [] },
        { "value": "Auto Repair & Garage", "subchoices": [] }
      ],
      "default_value": "all options",
      "conditional": {
        "show_when": {
          "facet": "search_type",
          "value": "Service Provider (QDA Member)"
        }
      }
    },
    {
      "id": "business_stage",
      "title": "Your business stage",
