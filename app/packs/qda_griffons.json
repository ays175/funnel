        { "value": "Established business (2-5 years)", "subchoices": ["Structuring", "Growing"] },
        { "value": "Mature business (> 5 years)", "subchoices": ["Expansion", "Diversification"] }
      ],
      "default_value": "all options"
    },
    {
      "id": "revenue_range",
